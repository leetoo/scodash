# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /                           controllers.Application.index
GET         /dashboard/:hash            controllers.Application.dashboard(hash)
GET         /ws/:hash                   controllers.Application.ws(hash)
GET         /sentiment/:symbol          controllers.StockSentiment.get(symbol)
POST        /create/                    controllers.Application.create
POST        /addItem/                   controllers.Application.addItem
GET         /createDashboard1           controllers.Application.createDashboard1
POST        /createDashboard2           controllers.Application.createDashboard2
POST        /addItem                    controllers.Application.addItem


# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file               controllers.Assets.at(path="/public", file)

    